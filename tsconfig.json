{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/solid/*": ["src/components/solidjs/*"],
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
